    "immutable": "3.8.1",
    "jcatalog-react-ui-autocompletes": "0.0.8",
    "jcatalog-react-ui-buttons": "1.0.1",
    "jcatalog-react-ui-svg": "0.0.11",
    "jcatalog-svg-icons": "0.0.4",
    "keycode": "2.1.7",
    "react-motion": "0.4.5"
