    "eslint-plugin-react": "5.1.1",
    "express": "4.13.4",
    "file-loader": "0.9.0",
    "jcatalog-dev-tools": "^2.0.15-SNAPSHOT.20161003T064012.880Z",
    "jcatalog-showroom": "1.2.11",
    "jcatalog-showroom-server": "1.0.1",
    "json-loader": "0.5.4",
    "less": "2.3.1",
    "less-loader": "2.2.2",
    "nodemon": "1.9.1",
    "postcss-loader": "0.13.0",
    "raw-loader": "0.5.1",
