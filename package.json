{
  "name": "@opuscapita/react-rich-editors",
  "version": "1.0.11",
  "description": "JS React based rich editors based on Draft.js",
  "scripts": {
    "link-mode": "webpack --watch --config ./webpack.link.config.js",
    "lint": "eslint src/client",
    "start": "nodemon --watch src/server src/server/index --exec babel-node --presets es2015,stage-0,react --plugins transform-decorators-legacy",
    "grails-plugin-build": "rimraf build && webpack --config ./webpack.production.config.js --progress --profile --colors",
    "grails-plugin-package": "npm run grails-plugin-build && grails-plugin-package",
    "grails-plugin-install": "npm run grails-plugin-package && grails-plugin-install",
    "grails-plugin-deploy": "npm run grails-plugin-package && grails-plugin-deploy",
    "grails-plugin-deploy-release": "npm run grails-plugin-package -- --release && grails-plugin-deploy --release",
    "npm-build": "rimraf lib && babel --copy-files --no-babelrc --presets es2015,stage-0,react --plugins transform-decorators-legacy --ignore __tests__,*.spec.js,demo src/client --out-dir lib",
    "npm-publish": "npm run npm-build && npm-publish",
    "publish-snapshot": "npm run grails-plugin-deploy && npm run npm-publish",
    "publish-release": "npm run grails-plugin-deploy-release && npm run npm-publish -- --release"
  },
  "main": "lib/index.js",
  "files": [
    "lib",
    "README.md"
  ],
  "license": "Apache-2.0",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "repository": "OpusCapita/react-rich-editors",
  "devDependencies": {
    "@opuscapita/npm-scripts": "1.0.8",
    "@opuscapita/react-showroom-client": "1.2.2",
    "@opuscapita/react-showroom-server": "1.2.0",
    "autoprefixer": "6.4.1",
    "babel-cli": "6.5.1",
    "babel-core": "6.4.0",
    "babel-eslint": "7.1.1",
    "babel-loader": "6.2.1",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-object-assign": "6.8.0",
    "babel-polyfill": "6.16.0",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-react": "6.3.13",
    "babel-preset-stage-0": "6.3.13",
    "body-parser": "1.15.2",
    "compression": "1.6.2",
    "cookie-parser": "1.4.3",
    "cross-env": "1.0.7",
    "css-loader": "0.21.0",
    "eslint": "3.11.1",
    "eslint-config-opuscapita": "1.0.3",
    "eslint-plugin-react": "6.7.1",
    "express": "4.13.4",
    "file-loader": "0.9.0",
    "intel": "1.1.0",
    "json-loader": "0.5.4",
    "less": "2.3.1",
    "less-loader": "2.2.2",
    "morgan": "1.7.0",
    "nodemon": "1.9.1",
    "postcss-loader": "0.13.0",
    "raw-loader": "0.5.1",
    "rimraf": "2.5.2",
    "source-map-loader": "0.1.5",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "1.12.2",
    "webpack-dev-middleware": "1.5.1",
    "write-file-webpack-plugin": "3.3.0"
  },
  "dependencies": {
    "@opuscapita/react-autocompletes": "1.0.8",
    "@opuscapita/react-buttons": "1.0.14",
    "@opuscapita/react-overlays": "1.0.14",
    "@opuscapita/svg-icons": "1.0.6",
    "draft-js": "0.9.1",
    "draft-js-export-html": "0.5.1",
    "draft-js-export-markdown": "^0.2.2",
    "draft-js-import-markdown": "^0.2.1",
    "draft-js-typeahead": "^0.1.1",
    "immutable": "3.8.1",
    "keycode": "2.1.7",
    "react": "0.14.8",
    "react-dom": "0.14.8",
    "react-motion": "0.4.5"
  },
  "grails": {
    "resources": {
      "react-rich-editors": {
        "files": {
          "client/rich-editors.bundle.min.js": "js/rich-editors.bundle.min.js"
        }
      }
    }
  }
}
